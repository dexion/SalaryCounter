<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.ShowTabel.TabelNameFragment">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:padding="@dimen/fab_margin">

        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/tabel_start_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/tabel_start_date_hint"
            app:met_floatingLabel="normal"
            app:met_floatingLabelText="@string/tabel_start_date"
            android:singleLine="true"
            android:inputType="none" />

        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/tabel_hours"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/tabel_hours_hint"
            app:met_floatingLabel="normal"
            app:met_floatingLabelText="@string/tabel_hours"
            android:singleLine="true"
            android:inputType="numberDecimal" />

    </LinearLayout>

</FrameLayout>
